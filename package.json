{
  "name": "qs-to-mongo",
  "version": "3.0.0",
  "description": "Convert query string parameters into mongo query filter and options.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Maksim Sinik <maksim@sinik.it>",
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "npm run lint && npm run clean-build",
    "build": "tsc",
    "clean-build": "rimraf ./dist && mkdir dist",
    "update": "npx npm-check -u",
    "lint": "biome check --apply .",
    "prepublishOnly": "npm run build",
    "test": "tsx --test ./test/*.ts",
    "test:watch": "tsx watch --test ./test/*.ts",
    "test:coverage": "c8 --all --src src --reporter lcov --reporter text tsx --test ./test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:fox1t/qs-to-mongo.git"
  },
  "keywords": [
    "querystring",
    "qs",
    "query",
    "url",
    "parse",
    "filter",
    "options",
    "criteria",
    "search",
    "MongoDB"
  ],
  "bugs": {
    "url": "https://github.com/fox1t/qs-to-mongo/issues"
  },
  "homepage": "https://github.com/fox1t/qs-to-mongo#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.12.12",
    "@types/qs": "^6.9.7",
    "c8": "^9.1.0",
    "husky": "^2.1.0",
    "rimraf": "^3.0.2",
    "tsx": "^4.10.4",
    "typescript": "^5.4.5"
  },
  "files": ["/lib", "/typings"],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run build"
    }
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bson-objectid": "^2.0.4",
    "qs": "^6.12.1",
    "tslib": "^2.6.2"
  }
}
